{
  "expo": {
    "name": "PokBills",
    "slug": "pokbills",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mypokbillsappng.mypokbillsappng"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mypokbillsappng.mypokbillsappng",
      "versionCode": 2
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "expo.modules": [
      {
        "name": "expo-document-picker",
        "version": "11.2.2"
      }
    ],
    "extra": {
      "oneSignalAppId": "d0aef24b-c16e-4dfc-a695-3dc9955c8457",
      "eas": {
        "build": {
          "experimental": {
            "ios": {
              "appExtensions": [
                {
                  "targetName": "OneSignalNotificationServiceExtension",
                  "bundleIdentifier": "com.mypokbillsappng.mypokbillsappng.OneSignalNotificationServiceExtension",
                  "entitlements": {
                    "com.apple.security.application-groups": [
                      "group.com.mypokbillsappng.mypokbillsappng.onesignal"
                    ]
                  }
                }
              ]
            }
          }
        },
        "projectId": "1a9ebc57-8f27-4792-94f8-a07b9a5c351e"
      }
    },
    "plugins": [
      [
        "onesignal-expo-plugin",
        {
          "mode": "production"
        }
      ]
    ]
  }
}
